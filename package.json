{
  "name": "nestjs-mcp-server",
  "version": "1.0.0",
  "description": "NestJSベースのMCPサーバー実装サンプル",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "tsc-watch --onSuccess \"node dist/main.js\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:integration": "vitest run test/integration",
    "test:e2e": "node test/e2e/test-client.mjs"
  },
  "keywords": [
    "mcp",
    "server",
    "nestjs",
    "ddd"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@rekog/mcp-nest": "^1.0.14",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
